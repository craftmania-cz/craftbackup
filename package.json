{
  "name": "craftbackup",
  "version": "1.0.0",
  "description": "Service for backups our servers and MySQL.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build-ci": "tsc -p tsconfig.json",
    "clean": "rm -rf dist/* && touch dist/.gitkeep",
    "start": "node dist/server.js",
    "watch": "nodemon dist/server.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "develop": "npm run clean && npm run build && npm run watch",
    "changelog": "auto-changelog --template keepachangelog --commit-limit false"
  },
  "keywords": [
    "node",
    "backup",
    "mysql",
    "ftp"
  ],
  "author": "MrWakeCZ",
  "license": "Apache 2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "0.0.38",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.11",
    "@types/js-yaml": "^4.0.1",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.10.1",
    "@types/node-cron": "^3.0.1",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "basic-ftp": "^4.6.6",
    "config": "^3.3.6",
    "dayjs": "^1.10.4",
    "debug": "^4.3.1",
    "discord-webhook-node": "^1.1.8",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.1.0",
    "knex": "^0.95.4",
    "moment": "^2.29.1",
    "mysql2": "^2.3.0",
    "mysqldump": "^3.2.0",
    "node-cron": "^3.0.0",
    "nodemon": "^2.0.7",
    "npm": "^7.11.2",
    "tslint": "^6.1.3",
    "winston": "^3.3.3"
  }
}
